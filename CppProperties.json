{
  "configurations": [
    {
      "inheritEnvironments": [
        "msvc_x64"
      ],
      "name": "x64-Debug",
      "includePath": [
        "${env.INCLUDE}",
        "${workspaceRoot}\\**",
        "${workspaceRoot}\\..\\hello-msvc\\**"
      ],
      "defines": [
        "WIN32",
        "_DEBUG",
        "UNICODE",
        "_UNICODE"
      ],
      "intelliSenseMode": "windows-msvc-x64",
      "compilerSwitches": "/std:c++latest",
      "forcedInclude": [
        "hello/stdafx.hxx"
      ]
    },
    {
      "inheritEnvironments": [
        "msvc_x64"
      ],
      "name": "x64-Release",
      "includePath": [
        "${env.INCLUDE}",
        "${workspaceRoot}\\**",
        "${workspaceRoot}\\..\\hello-msvc\\**"
      ],
      "defines": [
        "WIN32",
        "NDEBUG",
        "UNICODE",
        "_UNICODE"
      ],
      "intelliSenseMode": "windows-msvc-x64",
      "compilerSwitches": "/std:c++latest",
      "forcedInclude": [
        "hello/stdafx.hxx"
      ]
    }
  ]
}
